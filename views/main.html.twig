<!DOCTYPE html>
<html>
    <head>
        <title> {% block title %}Titre{% endblock %}</title>
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
        {% block stylesheets %}
        <link rel="stylesheet" type="text/css" href="{{WEBROOT}}templates/css/form.css"/>
        <script src="{{WEBROOT}}templates/js/jquery-1.8.3.js"></script>
        <script src="{{WEBROOT}}templates/js/jquery-ui.js"></script>
        <link rel="stylesheet" type="text/css" href="{{WEBROOT}}templates/css/main.css"/>
        <link rel="stylesheet" type="text/css" href="{{WEBROOT}}templates/css/content.css"/>
        <script>
            $(function() {
                $( "#accordion" ).accordion({
                    heightStyle: "content",
                    autoHeight: false,
                    navigation: true
                });
            });
        </script>
        {% endblock %}
    </head>
    <body>
        <header class="top">
            {% block header %}
                <h1>Gestionnaire de traitements médicaux</h1>
            {% endblock %}
        </header>
        <section>
        {% block body %}
        {% endblock %}

        </section>
        <footer>
        {% block footer %}
            Réalisé par David Charpentier, Hanane Ayadi et Quentin Bisson
        {% endblock %}
        </footer>
    </body>
</html>
